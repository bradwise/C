#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main () {
   float val;
   char str[20];
   
   strcpy(str, ".000005");
   val = atof(str);
   printf("String value = %s, Float value = %f\n", str, val);

    return(0);
}
//https://www.tutorialspoint.com/c_standard_library/c_function_atof.htm